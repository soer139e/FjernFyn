<Window x:Class="fjernfyn.Views.AdminDashBoardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fjernfyn.Views"
        mc:Ignorable="d"
        Title="AdminDashBoardWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Button Content="Opret Forespørgsel"
                Grid.Column="0"
                Grid.Row="0"
                Margin="80,80,80,80"
                FontSize="20"
                Command="{Binding CreateFeedbackCommand}" />
        <Button Content="Vis Forespørgsel"
                Grid.Column="1"
                Grid.Row="0"
                Margin="80,80,80,80"
                FontSize="20"
                Command="{Binding CreateFeedbackCommand}" />
        <Button Content="Udførte Forespørgsler"
                Grid.Column="0"
                Grid.Row="1"
                Margin="80,80,80,80"
                FontSize="20"
                Command="{Binding CreateFeedbackCommand}" />
        <Button Content="Admin"
                Grid.Column="1"
                Grid.Row="1"
                Margin="80,80,80,80"
                FontSize="20"
                Command="{Binding CreateFeedbackCommand}" />
    </Grid>
</Window>
